{
    "name": "emaillistverify/emaillistverify-sdk",
    "description": "PHP SDK for EmailListVerify API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "EmailListVerify",
            "email": "support@emaillistverify.com"
        }
    ],
    "homepage": "https://github.com/EmailListVerify-com/emaillistverify-sdk-php",
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "phpstan/phpstan": "^1.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "EmailListVerify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EmailListVerify\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse src --level=5",
        "phpcs": "phpcs --standard=PSR12 src/",
        "phpcs-fix": "phpcbf --standard=PSR12 src/"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}